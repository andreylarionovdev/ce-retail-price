{% block ce_retail_price_list %}
    <sw-page class="ce-retail-price-list">
        {% block ce_retail_price_list_smart_bar_actions %}
            <template slot="smart-bar-actions">
                <sw-button variant="primary" :routerLink="{ name: 'ce.retail.price.create' }">
                    {{ $t('ce-retail-price.list.addButtonText') }}
                </sw-button>
            </template>
        {% endblock %}

        <template slot="content">
            {% block ce_retail_price_list_content %}
                <sw-entity-listing
                    v-if="entities"
                    :items="entities"
                    :repository="repository"
                    :showSelection="false"
                    :columns="columns"
                    detailRoute="ce.retail.price.detail">
                </sw-entity-listing>
            {% endblock %}
        </template>
    </sw-page>
{% endblock %}
